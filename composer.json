{
  "name": "jaspersoft/jasper-rest-client",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Jaspersoft",
      "email": "code@jaspersoft.com"
    },
    {
      "name": "ERAMON GmbH",
      "email": "info@eramon.de"
    }
  ],
  "homepage": "http://github.com/Jaspersoft/jrs-rest-php-client",
  "keywords": [
    "jasperreports",
    "jaspersoft",
    "jasper-reports",
    "reporting"
  ],
  "require": {
    "php": ">=7.1",
    "symfony/http-client": "^4.3",
    "symfony/http-kernel": "^4.3",
    "ext-curl": "*",
    "symfony/framework-bundle": "^4.3",
    "symfony/yaml": "^4.3",
    "symfony/twig-bundle": "^4.3"
  },
  "require-dev": {
    "symfony/browser-kit": "*",
    "symfony/css-selector": "*",
    "symfony/debug-bundle": "*",
    "symfony/maker-bundle": "^1.11",
    "symfony/phpunit-bridge": "*",
    "symfony/stopwatch": "*",
    "symfony/web-profiler-bundle": "*",
    "symfony/web-server-bundle": "*",
    "phpunit/phpunit": "^7.5"
  },
  "type": "symfony-bundle",
  "description": "A Symfony HttpClient Bundle for the JasperReports Server REST API",
  "autoload": {
    "psr-4": {
      "Jasper\\RestClient\\": "./",
      "Jaspersoft\\": "./src/Jaspersoft/"
    },
    "classmap": [
      "./AppKernel.php"
    ],
    "exclude-from-classmap": [
      "tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Jasper\\RestClient\\Tests\\Phpunit\\": "./Phpunit",
      "Jaspersoft\\Tests\\": "./tests/"
    }
  },
  "config": {
    "discard-changes": true,
    "platform": {
      "php": "7.1.8"
    },
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
